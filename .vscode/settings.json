{
  "eslint.validate": ["javascript", "javascriptreact", "typescript"],
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": true,
    "strings": true
  }
}
